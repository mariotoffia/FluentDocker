{
    "dotnet.defaultSolution": "FluentDocker.sln",
    "omnisharp.useModernNet": true,
    "omnisharp.enableEditorConfigSupport": true,
    "csharp.format.enable": true,
    "editor.formatOnType": true,
    "dotnet.server.useOmnisharp": true,
    "omnisharp.enableMsBuildLoadProjectsOnDemand": false,
    "omnisharp.maxProjectResults": 1000,
    "omnisharp.useEditorFormattingSettings": true,
    "dotnet.backgroundAnalysis.analyzerDiagnosticsScope": "openFiles",
    "dotnet-test-explorer.testProjectPath": "**/*Tests.csproj",
    "dotnet-test-explorer.testArguments": "-f net8.0 --logger \"console;verbosity=detailed\"",
    "dotnet-test-explorer.autoWatch": false,
    "dotnet-test-explorer.autoExpandTree": true,
    "dotnet-test-explorer.showCodeLens": true,
    "dotnet-test-explorer.codeLensFailed": "❌",
    "dotnet-test-explorer.codeLensPassed": "✅",
    "dotnet-test-explorer.runInParallel": false,
    "dotnet-test-explorer.treeMode": "full",
    "dotnet.completion.showCompletionItemsFromUnimportedNamespaces": true,
    "dotnet.formatting.organizeImportsOnFormat": true
}